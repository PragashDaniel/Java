//Another solution
public class Fahrenheit {
    
    float cel;
    Fahrenheit(float c){
        cel=c;
        float F=(c*9/5)+32;
        System.out.println("Fahrenheit :"+F);
    }
    
    public static void main(String[] args) {
        Scanner dany=new Scanner(System.in);
        float num=dany.nextFloat();
        Fahrenheit obj=new Fahrenheit(num);
    }
    
}

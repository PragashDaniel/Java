//Another solution
public class Fahrenheit {
    public static void main(String[] args) {
        Scanner dany=new Scanner(System.in);
        int num=dany.nextInt();
        Fahrenheit obj = new Fahrenheit();
        float F=obj.calculation(num);
        System.out.println("Fahrenheit :"+F);
    }
    
    float calculation(float C) { 
        return C*9/5+32;
        
    }
    
}

public class Exercise6 {
    public static void main(String[] args) {
        Exercise6 obj = new Exercise6();
        Random dany = new Random();
        int random=dany.nextInt(100);
        System.out.println("The random number is :"+random);
        float result=obj.random(random);
        
    }
    float random(int randomno){
        int[] arr=new int[1];
        arr[0]=randomno;
        float result=1,i;
        for(i=1;i<=arr[0];i++){
            result=result*i;
            System.out.println(""+result);
        }
        return result;
    }
}

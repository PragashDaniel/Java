//Conversion
public class Conversion {

    public static void main(String[] args) {
    
       Conversion obj =new Conversion();
       float a=obj.inttofloat();
       System.out.println("Integer to Float :"+a);
       int b=obj.floattoint();
       System.out.println("Float to Integer :"+b);
       float c=obj.stringtofloat();
       System.out.println("String to Float :"+c);
       int d=obj.stringtoint();
       System.out.println("String to Integer :"+d);
       String e=obj.floattostring();
       System.out.println("Float to String :"+e);
       String f=obj.inttostring();
       System.out.println("Integer to Float :"+f);
    }
    
    float inttofloat() {
        int a=10;
        float b=(int)a;
        return b;
    }
    
    int floattoint() {
        float a=20.5f;
        int b=(int)a;
        return b;
    }
    
    float stringtofloat(){
        String a="500";
        float b=Float.parseFloat(a);
        return b;
    }
     
    int stringtoint(){
        String a="500";
        int b=Integer.parseInt(a);
        return b;
    }
    String floattostring(){
        float a=12.5f;
        String b=String.valueOf(a);
        return b;
    }
    String inttostring() {
        int a=10;
        String b=String.valueOf(a);
        return b;
    }
}
